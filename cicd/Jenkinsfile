pipeline{
 agent any
 environment {
    ANYPOINT = credentials('ANYPOINT')
 }
 stages {
 	stage ('Build'){
 		steps {
 			withMaven(maven:'maven'){
 				sh 'mvn -f cicd/pom.xml clean install'
 			}
 		}
 	}
 	stage ('Deploy'){
 		steps {
 			withMaven(maven:'maven'){
 				sh 'mvn -f cicd/pom.xml package deploy  -Dusername=JenkinsDevOps -Dpassword=DevOps1234@ -Denvironment=Sandbox -DmuleDeploy'
 			}
 		}
 	}
 }

}